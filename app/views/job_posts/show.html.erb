<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @job_post.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @job_post.description %>
</p>

<p>
  <strong>Date:</strong>
  <%= @job_post.date %>
</p>

<p>
  <strong>Posted by:</strong>
  <%= @job_post.poster_name %>
</p>

<ul>
  <% @job_post.tags.each do |tag| %>
  <li><%= tag.name %></li>
  <% end %>
</ul>

<%= link_to 'Edit', edit_job_post_path(@job_post) %> |
<%= link_to 'All Job Posts', job_posts_path %>
